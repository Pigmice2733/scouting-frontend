    "preact-router": "^2.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.44",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.44",
    "@babel/plugin-transform-react-jsx": "^7.0.0-beta.44",
    "@babel/plugin-transform-typescript": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@types/jest": "^22.2.3",
    "autoprefixer": "^8.3.0",
    "connect-history-api-fallback": "^1.5.0",
    "cross-env": "^5.1.4",
    "greenkeeper-lockfile": "^1.14.0",
    "husky": "^1.0.0-rc.1",
    "jest": "^22.4.3",
    "koa-connect": "^2.0.1",
    "postcss-calc": "^6.0.1",
    "postcss-color-function": "^4.0.1",
    "postcss-color-gray": "^4.1.0",
    "postcss-css-variables": "^0.8.1",
    "postcss-media-minmax": "^3.0.0",
    "postcss-nesting": "^5.0.0",
    "rollup": "^0.58.2",
    "rollup-plugin-babel": "^4.0.0-beta.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-generate-html-template": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-postcss": "^1.6.1",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-plugin-uglify": "^3.0.0",
    "rucksack-css": "^1.0.2",
    "sugarss": "^1.0.1",
    "ts-jest": "^22.4.4",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.10.0",
    "typescript": "^2.8.1"
  },
  "license": "MIT",
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-serve",
    "build": "cross-env NODE_ENV=production webpack",
    "type": "tsc --noEmit --pretty",
    "type:watch": "tsc --noEmit --pretty --watch",
    "lint": "tslint --project tsconfig.json --format codeFrame",
    "fix": "prettier --write 'src/**.ts*' && yarn lint --fix",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "jest": {
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "transform": {
      "^.+\\.tsx$": "ts-jest",
      "^.+\\.ts$": "ts-jest",
      "^.+\\.jsx$": "ts-jest",
      "^.+\\.js$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "tslint --project tsconfig.json --format codeFrame"
    }
  }
}
