.adminPanel
  display: flex
  flex-direction: column
  align-items: center
  
  & table
    margin: 1em auto 0 auto
    border-collapse: collapse

    & td
      padding: 0.4em 1em

    & button
      display: flex
      justify-content: center
      align-items: center
      
      & svg
        width: 1em
        height: 1em

.del
  background: #ff4e4e
  padding: 0
  width: 2em
  height: 2em

.success
  background: rgba(152, 251, 152, 0.3)

.failed
  background: red